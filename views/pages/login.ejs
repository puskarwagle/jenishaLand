<head>
  <link rel="stylesheet" href="./css/login.css">
</head>

<body>
<div id="login">
  <form action="/login/password" name="form" method="post">
  	  <section>
    	    <label for="username">Username:</label>
      	  <input onclick="resetBtn()" id="username" name="username" type="text" autocomplete="username" required autofocus>
    	</section>
    	<section>
      	  <label for="password">Password:</label>
        	<input onclick="resetBtn()" id="password" name="password" type="password" autocomplete="current-password" required>
    	</section>
    	<button id="submit" onmouseover="mouseOver()" type="submit">Sign in</button>
	</form>
</div>
<script src="../js/button.js"></script>
</body>

